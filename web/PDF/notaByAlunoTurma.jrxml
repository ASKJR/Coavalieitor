	<queryString language="SQL">
		<![CDATA[SELECT aval.nome AS avaliacao,cf.feedback,ROUND(cf.nota_final,2) AS nota ,cf.correcao_final_data,tur.nome AS turma, 
us.nome AS professor,usAluno.nome AS aluno,dis.nome AS disciplina,cur.nome AS curso,inst.nome AS instituicao
FROM correcao_final cf
JOIN avaliacao aval ON (cf.avaliacao_id = aval.id)
JOIN turma tur ON (aval.turma_id = tur.id)
JOIN disciplina dis ON (tur.disciplina_id = dis.id)
JOIN curso cur ON (dis.curso_id = cur.id)
JOIN instituicao inst ON (cur.instituicao_id = inst.id)
JOIN usuario us ON (tur.professor_usuario_id = us.id)
JOIN usuario usAluno ON (usAluno.id = cf.aluno_usuario_id)
WHERE tur.id = $P{turma_id} 
AND cf.aluno_usuario_id = $P{aluno_id} 
AND cf.correcao_visivel = true]]>
	</queryString>
